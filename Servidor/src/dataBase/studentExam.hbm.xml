<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="entities.StudentExam" table="StudentExam">  
    <id name="id">  
    	<generator class="increment"></generator>  
    </id>  

    <property name="studentId" type="int"></property>
    <property name="unityId" type="int"></property>
    <property name="sessionId" type="int"></property>
    <property name="result" ></property>
    <property name="isFinal" ></property>
    </class>  
            
 </hibernate-mapping>  
