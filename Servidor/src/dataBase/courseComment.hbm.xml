<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="entities.CourseComment" table="CourseComment">  
    <id name="id">  
    	<generator class="increment"></generator>  
    </id>  
 	
   	<property name="courseId" type="int"></property>
	<property name="studentId" type="int"></property>
   	<property name="message" ></property>
	<property name="time" type="time"></property>
   </class>  
            
 </hibernate-mapping>  
